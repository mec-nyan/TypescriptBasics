{
  "name": "TypescriptBasics",
  "version": "1.0.0",
  "description": "Here you'll find little projects/files I'm writing while learning typescript.",
  "main": "index.js",
  "scripts": {
    "start": "node --inspect=5858 -r ts-node/register ./src/app.ts",
    "start:watch": "nodemon",
    "build": "tsc",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "nodemonConfig": {
    "ignore": [
      "**/*.test.ts",
      "**/*.spect.ts",
      ".git",
      "node_modules"
    ],
    "watch": [
      "src"
    ],
    "exec": "npm start",
    "ext": "ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mec-nyan/TypescriptBasics.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/mec-nyan/TypescriptBasics/issues"
  },
  "homepage": "https://github.com/mec-nyan/TypescriptBasics#readme",
  "dependencies": {
    "express": "^4.17.1"
  },
  "devDependencies": {
    "@types/node": "^16.11.7",
    "eslint": "^8.2.0",
    "eslint-plugin-import": "^2.25.2",
    "nodemon": "^2.0.14",
    "ts-node": "^10.4.0",
    "typescript": "^4.4.4"
  }
}
